{
    "name": "@umatch/utils",
    "version": "1.23.3",
    "description": "Math, array, string, object &amp; time-related functions and types.",
    "author": "Gabriel Okamoto <gabrielokamoto@hotmail.com>",
    "license": "MIT",
    "keywords": [
        "utils",
        "typescript"
    ],
    "homepage": "https://github.com/umatch-oficial/utils#readme",
    "repository": "https://github.com/umatch-oficial/utils.git",
    "bugs": "https://github.com/umatch-oficial/utils/issues",
    "main": "./lib/index.js",
    "files": [
        "lib/**/*.ts",
        "lib/**/*.ts.map",
        "lib/**/*.js"
    ],
    "exports": {
        ".": "./lib/index.js",
        "./array": "./lib/array/index.js",
        "./math": "./lib/math/index.js",
        "./object": "./lib/object/index.js",
        "./string": "./lib/string/index.js",
        "./time": "./lib/time/index.js"
    },
    "typesVersions": {
        "*": {
            "array": [
                "lib/array"
            ],
            "math": [
                "lib/math"
            ],
            "object": [
                "lib/object"
            ],
            "string": [
                "lib/string"
            ],
            "time": [
                "lib/time"
            ]
        }
    },
    "scripts": {
        "build": "tsc --build src",
        "test": "jest",
        "lint": "eslint . --ext=.ts",
        "prepublishOnly": "npm run lint && npm run build && npm test",
        "prepare": "npm run build",
        "preversion": "npm run lint && npm run build && npm test",
        "postversion": "git push && git push --tags",
        "patch-and-publish": "npm version patch && npm publish --ignore-scripts",
        "minor-and-publish": "npm version minor && npm publish --ignore-scripts",
        "major-and-publish": "npm version major && npm publish --ignore-scripts"
    },
    "dependencies": {
        "bluebird": "^3.7.2",
        "chalk": "^4.1.2",
        "luxon": "^3.1.0",
        "string-length": "^4.0.2"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.38",
        "@types/jest": "^29.2.4",
        "@types/luxon": "^3.1.0",
        "@types/node": "^18.11.15",
        "@typescript-eslint/eslint-plugin": "^5.46.1",
        "@typescript-eslint/parser": "^5.46.1",
        "commitizen": "^4.2.6",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "~8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-typescript": "^3.5.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.3.1",
        "prettier": "^2.8.1",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
