{
    "name": "@umatch/utils",
    "version": "9.0.1",
    "description": "Math, array, string, object &amp; time-related functions and types.",
    "author": "Gabriel Okamoto <gabrielokamoto@hotmail.com>",
    "license": "MIT",
    "keywords": [
        "utils",
        "typescript"
    ],
    "homepage": "https://github.com/umatch-oficial/utils#readme",
    "repository": "https://github.com/umatch-oficial/utils.git",
    "bugs": "https://github.com/umatch-oficial/utils/issues",
    "main": "./lib/index.js",
    "files": [
        "src/**/*.ts",
        "lib/**/*.ts",
        "lib/**/*.ts.map",
        "lib/**/*.js"
    ],
    "exports": {
        ".": "./lib/index.js",
        "./array": "./lib/array/index.js",
        "./math": "./lib/math/index.js",
        "./object": "./lib/object/index.js",
        "./string": "./lib/string/index.js",
        "./time": "./lib/time/index.js"
    },
    "typesVersions": {
        "*": {
            "array": [
                "lib/array"
            ],
            "math": [
                "lib/math"
            ],
            "object": [
                "lib/object"
            ],
            "string": [
                "lib/string"
            ],
            "time": [
                "lib/time"
            ]
        }
    },
    "scripts": {
        "build": "tsc --build src",
        "test": "jest",
        "lint": "eslint . --ext=.ts",
        "prepublishOnly": "npm run lint && npm run build && npm test",
        "prepare": "npm run build",
        "push-publish": "git push --follow-tags && npm publish --ignore-scripts",
        "release": "npm run lint && npm run build && npm test && commit-and-tag-version"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "bluebird": "^3.7.2",
        "chalk": "^4.1.2",
        "luxon": "^3.3.0",
        "string-length": "^4.0.2"
    },
    "devDependencies": {
        "@types/bluebird": "^3.5.38",
        "@types/jest": "^29.5.2",
        "@types/luxon": "^3.3.0",
        "@types/node": "^18.16.13",
        "@typescript-eslint/eslint-plugin": "^5.59.9",
        "@typescript-eslint/parser": "^5.59.9",
        "@umatch/eslint-config": "^1.2.0",
        "commit-and-tag-version": "^11.2.1",
        "commitizen": "^4.3.0",
        "csv-parser": "^3.0.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.42.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-import-resolver-typescript": "^3.5.5",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.5.0",
        "prettier": "^2.8.8",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
    }
}
