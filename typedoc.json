{
  "$schema": "https://typedoc-plugin-markdown.org/schema.json",
  "name": "@umatch/utils",
  // typedoc options
  "classPropertiesFormat": "table",
  "excludeExternals": true,
  "gitRevision": "main",
  "readme": "README-base.md",
  "entryPoints": [
    "src",
    "src/array",
    "src/csv",
    "src/math",
    "src/object",
    "src/string",
    "src/time"
  ],
  // plugins
  "plugin": [
    "typedoc-plugin-markdown",
    "typedoc-plugin-missing-exports",
    "typedoc-plugin-rename-defaults"
  ],
  // plugin-markdown options
  "modulesFileName": "api",
  "expandObjects": true,
  "useCodeBlocks": true,
  "textContentMappings": {
    "header.title": "About this project"
  }
}
